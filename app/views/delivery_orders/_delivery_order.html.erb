<tr data-delivery-order="<%= delivery_order.id %>">
  <% unless do_not_show_schedule_button %>
    <td><input type="checkbox" class="deliveryOrderCheckbox" /></td>
  <% end %>
  <td><%= delivery_type(delivery_order) %></td>
  <td><%= distance_of_time_in_words(delivery_order.created_at, Time.zone.now) %> ago</td>
  <td><%= delivery_order.title.title %></td>
  <td><%= delivery_order.ready_for_processing? %></td>
  <td><%= delivery_slot(delivery_order) %></td>
  <td>
    <%= delivery_order.delivery_notes.size %>
    <a class="show_notes_button" target="#">View</a>
  </td>
  <td>
    <%= form_tag delivery_order_closure_path(delivery_order) do %>
      <%= submit_tag "Close" %>
    <% end %>
  </td>
</tr>