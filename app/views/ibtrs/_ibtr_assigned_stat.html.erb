	<td class="span-2"><%= link_to ibtr_assigned_stat.poplaced_cnt, ibtrs_drillrpt_path(:report => params[:report], :branch_id => ibtr_assigned_stat.respondent_id, :state => 'POPlaced', :Created=>params[:Created], :start=>params[:start], :end => params[:end] ) %><br/></td>
