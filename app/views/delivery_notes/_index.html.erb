<div style="float: right"><a class="hide_notes_button" href="">X</a></div>
<table class="delivery_notes">
  <tr>
    <td class="heading">Time</td>
    <td class="heading">Comment</td>
  </tr>
  <% @notes.each do |note| %>
    <tr>
      <td><%= note.created_at.strftime("%b %d %Y") %></td>
      <td><%= note.content %></td>
    </tr>
  <% end %>
</table>